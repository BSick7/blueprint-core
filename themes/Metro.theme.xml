<ResourceDictionary
        xmlns="http://schemas.wsick.com/fayde"
        xmlns:x="http://schemas.wsick.com/fayde/x"
        xmlns:blueprint="lib://blueprint-core">
    <Style TargetType="blueprint:Container">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="blueprint:Container">
                    <Canvas x:Name="Canvas">
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="blueprint:Resource">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="blueprint:Resource">
                    <Image Source="{TemplateBinding ImageSource}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="blueprint:Link">
        <Setter Property="Stroke" Value="Black" />
        <Setter Property="StrokeThickness" Value="2" />
    </Style>
</ResourceDictionary>